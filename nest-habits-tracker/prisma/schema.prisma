datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Habit {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  isActive    Boolean  @default(true)
  createdAt   DateTime @default(now())
}
